Ddoc

Macros:
	H2 = <h2>$0</h2>
	BR = <br />
	DOLLAR = $
	COLON = &#58;
	EM       = $(B $(I $0) )

	TYPE_MAPPINGS = See the $(LINK2 $(DDOX_ROOT_DIR)/mysql.html, MySQL/D Type Mappings tables)
	SAFE_MIGRATION = See the $(LINK2 https://github.com/mysql-d/mysql-native/blob/master/SAFE_MIGRATION.md, Safe Migration) document for more details.

	STD_DATETIME_DATE = $(D_INLINECODE $(LINK2 https://dlang.org/phobos/std_datetime_date.html#$0, $0))
	STD_EXCEPTION     = $(D_INLINECODE $(LINK2 https://dlang.org/phobos/std_exception.html#$0, $0))
	STD_FILE          = $(D_INLINECODE $(LINK2 https://dlang.org/phobos/std_file.html#$0, $0))
	STD_PATH          = $(D_INLINECODE $(LINK2 https://dlang.org/phobos/std_path.html#$0, $0))
	STD_PROCESS       = $(D_INLINECODE $(LINK2 https://dlang.org/phobos/std_process.html#$0, $0))
	STD_STDIO         = $(D_INLINECODE $(LINK2 https://dlang.org/phobos/std_stdio.html#$0, $0))
	FULL_STD_DATETIME_DATE = $(D_INLINECODE $(LINK2 https://dlang.org/phobos/std_datetime_date.html#$0, std.datetime.date.$0))
	FULL_STD_EXCEPTION     = $(D_INLINECODE $(LINK2 https://dlang.org/phobos/std_exception.html#$0, std.exception.$0))
	FULL_STD_FILE          = $(D_INLINECODE $(LINK2 https://dlang.org/phobos/std_file.html#$0, std.file.$0))
	FULL_STD_PATH          = $(D_INLINECODE $(LINK2 https://dlang.org/phobos/std_path.html#$0, std.path.$0))
	FULL_STD_PROCESS       = $(D_INLINECODE $(LINK2 https://dlang.org/phobos/std_process.html#$0, std.process.$0))
	FULL_STD_STDIO         = $(D_INLINECODE $(LINK2 https://dlang.org/phobos/std_stdio.html#$0, std.stdio.$0))
	DMD             = $(LINK2 https://dlang.org/, DMD)
	DDOX            = $(LINK2 https://github.com/rejectedsoftware/ddox, ddox)
	MODULE_STD_FILE = $(D_INLINECODE $(LINK2 https://dlang.org/phobos/std_file.html, std.file))
	MODULE_STD_PATH = $(D_INLINECODE $(LINK2 https://dlang.org/phobos/std_path.html, std.path))

	LICENSE = $(LINK2 https://github.com/Abscissa/mysql-native/blob/master/LICENSE.txt, zlib/libpng)
